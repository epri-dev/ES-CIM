<xs:element name="isFreeSequence" minOccurs="0" maxOccurs="1" type="xs:boolean" sawsdl:modelReference="http://iec.ch/TC57/CIM-schema-cim16#ProcedureStep.isFreeSequence">
<xs:annotation>
