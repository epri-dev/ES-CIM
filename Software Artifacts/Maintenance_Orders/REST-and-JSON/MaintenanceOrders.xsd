<xs:element name="addressKind" minOccurs="0" maxOccurs="1" type="m:AddressKind" sawsdl:modelReference="http://iec.ch/TC57/CIM-schema-cim16#Address.addressKind">
<xs:annotation/>
</xs:element>
<xs:element name="addressKindOther" minOccurs="0" maxOccurs="1" type="xs:string" sawsdl:modelReference="http://iec.ch/TC57/CIM-schema-cim16#Address.addressKindOther">
<xs:annotation/>
</xs:element>
<xs:element name="cityCode" minOccurs="0" maxOccurs="1" type="xs:string" sawsdl:modelReference="http://iec.ch/TC57/CIM-schema-cim16#Address.cityCode">
