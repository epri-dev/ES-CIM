</xs:annotation>
</xs:element>
<xs:element name="sequenceNumber" minOccurs="0" maxOccurs="1" type="xs:integer" sawsdl:modelReference="http://iec.ch/TC57/CIM-schema-cim16#ProcedureStep.sequenceNumber">
