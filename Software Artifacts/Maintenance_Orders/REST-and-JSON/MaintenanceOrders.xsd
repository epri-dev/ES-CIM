<xs:documentation>City code.</xs:documentation>
</xs:annotation>
</xs:element>
<xs:element name="cityName" minOccurs="0" maxOccurs="1" type="xs:string" sawsdl:modelReference="http://iec.ch/TC57/CIM-schema-cim16#Address.cityName">
